{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "udacityakinoladdf"
		},
		"Ln_azure_db_project_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'Ln_azure_db_project'"
		},
		"Ln_azuresynapse_project_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'Ln_azuresynapse_project'"
		},
		"link_to_synapse_datalake_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'link_to_synapse_datalake'"
		},
		"udacity_source_data_DL_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'udacity_source_data_DL'"
		},
		"link_to_synapse_datalake_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://udacityakinolassynapse.dfs.core.windows.net/"
		},
		"udacity_source_data_DL_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://udacityakinstorage.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "udacityakinoladdf_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"Ln_azure_db_project_connectionString": {
						"value": "[parameters('Ln_azure_db_project_connectionString')]"
					},
					"Ln_azuresynapse_project_connectionString": {
						"value": "[parameters('Ln_azuresynapse_project_connectionString')]"
					},
					"link_to_synapse_datalake_accountKey": {
						"value": "[parameters('link_to_synapse_datalake_accountKey')]"
					},
					"udacity_source_data_DL_accountKey": {
						"value": "[parameters('udacity_source_data_DL_accountKey')]"
					},
					"link_to_synapse_datalake_properties_typeProperties_url": {
						"value": "[parameters('link_to_synapse_datalake_properties_typeProperties_url')]"
					},
					"udacity_source_data_DL_properties_typeProperties_url": {
						"value": "[parameters('udacity_source_data_DL_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "udacityakinoladdf_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'udacityakinoladdf_ArmTemplate_0')]"
			]
		}
	]
}